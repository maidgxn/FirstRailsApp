<h2>Edit user profile</h2>

<%= form_with(model: @client, local: true) do |f| %>
  <div class="field">
    <%= f.label :email %>
    <%= f.email_field :email, value: @client.email, class:"form-control" %>
  </div>
<br>
  <div class="actions">
    <%= f.submit "Update", class:"btn btn-outline-success" %>
  </div>
<% end %>
<br>
<%= link_to "Back", clients_path, class: "btn btn-outline-info" %>

